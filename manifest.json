{
    "name": "Star Wars Scroller",
    "version": "0.0.3",
    "manifest_version": 3,
    "description": "Scroll your webpage like star wars.",
    "icons": {
        "16": "icons/DarthVader16.png",
        "48": "icons/DarthVader48.png",
        "128": "icons/DarthVader.png"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": [
            "src/bg/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icons/DarthVader.png",
        "default_title": "Star Wars Scroller"
    },
    "web_accessible_resources": [
    "src/inject/inject.css",
    "src/inject/space.jpg"
  ]
}
